{
  "name": "desktop-habitica",
  "productName": "Habitica",
  "version": "1.0.0",
  "description": "Unofficial Habitica desktop app",
  "main": "main.js",
  "scripts": {
    "start": "./node_modules/.bin/electron .",
    "build": "./node_modules/.bin/electron-packager . $npm_package_productName --overwrite --out=dist --ignore='^/dist$' --platform=darwin --arch=x64 --version=0.36.5 --app-version=$npm_package_version"
