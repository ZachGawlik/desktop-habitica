  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZachGawlik/desktop-habitica/issues"
  },
  "homepage": "https://github.com/ZachGawlik/desktop-habitica#readme",
  "devDependencies": {
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.36.5"
  }
}
